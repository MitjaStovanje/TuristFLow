    xmlns:local="using:TuristFlow"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Class="TuristFlow.MainPage"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <RadioButton x:Name="radioButton" Content="RadioButton" HorizontalAlignment="Left" Margin="107,287,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="107,324,0,0" VerticalAlignment="Top" Click="button_Click"/>
    </Grid>
</Page>
